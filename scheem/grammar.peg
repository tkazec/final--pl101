start
	= expression

char
	= [-0-9a-zA-Z_?!+=@#$%^&*/.]

spacechar
	= [ \t\r\n]*

space
	= spacechar (";;" (!"\n" .)* "\n" spacechar)*

atom
	= space v:char+ space
		{ return v.join(""); }

expression
	= space "'" v:expression space
		{ return ["quote", v]; }
	/ atom
	/ space "(" v:expression* ")" space
		{ return v; }